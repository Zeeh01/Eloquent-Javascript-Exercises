/*
I'm still working on the width solution. The height was somewhat easy to figure it out, but the width is making my brain hurt.
*/

let height = prompt("Type the height");
let width = prompt("Type the width");
for (let n = 1; n <= height; n += 1) {
  if (n % 2 == 0)
    console.log(" # # # #");
  else
    console.log("# # # #");
}
